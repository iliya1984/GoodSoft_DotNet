<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      autoReload="true"
      internalLogLevel="Info"
      internalLogFile="c:\temp\internal-nlog.txt">
<extensions>
    <add assembly="NLog.Web.AspNetCore"/>
  </extensions>
  <targets>
     <target xsi:type="File" name="logfile" fileName="/home/ilya/logfiles/${event-properties:item=CustomFileName}.${event-properties:item=Extension}"/>
  </targets>
  <rules>
    <logger name="*" minlevel="Trace" writeTo="logfile" />ubunt
  </rules>
</nlog>